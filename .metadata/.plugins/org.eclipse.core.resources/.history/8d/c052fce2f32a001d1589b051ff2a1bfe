package br.com.residencia.poo.exercicioSlide;

public class BalancoTrimestral {

	public static void main(String[] args) {

		
		int gastosJaneiro = 15000;
		int gastosFevereiro = 23000;
		int gastosMarco = 17000;
		int gastosTrimestre = gastosJaneiro+gastosFevereiro+gastosMarco;
		
		System.out.println("Os gastos Trimestrais s達o: "+gastosTrimestre);
		System.out.println("Os gastos de Janeiro s達o:"+gastosJaneiro);
		System.out.println("Os gastos de Janeiro s達o:"+gastosFevereiro);
		System.out.println("Os gastos de Janeiro s達o:"+gastosMarco);
		
	}

}
